            {
              "type": "canvas",
              "id": 1584443213114,
              "value": "      function draw(canvas, ctx, width, height) {\n        // 绘制背景\n        function background(color) {\n          ctx.fillStyle = color;\n          ctx.fillRect(0, 0, width, height);\n        }\n\n        // 全局变量\n        let mouseX = width / 2;\n        let mouseY = height / 2;\n        const size = width / 10;\n        const cnt = 50;\n        const circleArr = [];\n        for (let i = 0; i < cnt; i++) {\n          circleArr.push({\n            x: mouseX,\n            y: mouseX\n          });\n        }\n\n        function update() {\n          // shift array\n          for (let i = 0; i < cnt - 1; i++) {\n            circleArr[i].x = circleArr[i + 1].x;\n            circleArr[i].y = circleArr[i + 1].y;\n          }\n\n          circleArr[cnt - 1].x = mouseX;\n          circleArr[cnt - 1].y = mouseY;\n\n          // draw circle\n          background(\"black\");\n          circleArr.forEach((item, index) => {\n            const c = 255 - (cnt - index);\n            ctx.fillStyle = `rgb(${c},${c},${c})`;\n            ctx.beginPath();\n            ctx.arc(item.x, item.y, index, 0, 2 * Math.PI);\n            ctx.fill();\n          });\n        }\n\n        canvas.onmousemove = e => {\n          mouseX = e.offsetX;\n          mouseY = e.offsetY;\n        };\n\n        const timer = setInterval(update, 10);\n        return timer;\n      }",
              "attrs": { "padding": 0 }
            }
          ]
        }
      ]
