\n\n          circleArr[cnt - 1].x = mouseX;\n          circleArr[cnt - 1].y = mouseY;\n\n          // draw circle\n          background(\"black\");\n          circleArr.forEach((item, index) => {\n            const c = 255 - (cnt - index);\n            ctx.fillStyle = `rgb(${c},${c},${c})`;\n            ctx.beginPath();\n            ctx.arc(item.x, item.y, index, 0, 2 * Math.PI);\n            ctx.fill();\n          });\n        }\n\n        canvas.onmousemove = e => {\n          mouseX = e.offsetX;\n          mouseY = e.offsetY;\n        };\n\n        const timer = setInterval(update, 10);\n        return timer;\n      }","attrs":{"padding":0}}]}]},{"type":"panel","id":5,"value":"column","attrs":{"span":[1,2],"flex":"column","padding":"$6","backgroundColor":"$1"},"children":[{"id":"text5","value":"Meaningful switching animation","type":"text","attrs":{"fontSize":101,"color":"$5","fontWeight":"bold","textAlign":"center","verticalAlign":"center","padding":0,"isTitle":true}},{"type":"panel","id":"panel5","value":"row","attrs":{"span":[1,1,1],"flex":"row","padding":10},"children":[{"type":"panel","id":1585193526942,"value":"column","attrs":{"span":[0.53,1.47],"flex":"column","padding":10},"children":[{"id":1585193591554,"value":"\nAmplification : big view to small view","type":"text","attrs":{"fontSize":30,"color":"$5","fontWeight":"normal","textAlign":"center","verticalAlign":"center","padding":0,"isTitle":false}},{"type":"image","id":1584442424406,"value":"https://i.loli.net/2020/03/18/5pomxiVu9tE27df.gif","attrs":{"displayMode":"normal","padding":10,"textAlign":"center","verticalAlign":"center"},"_children":null}],"_children":null},{"type":"panel","id":1585193536883,"value":"column","attrs":{"span":[0.52,1.48],"flex":"column","padding":10},"children":[{"id":1585193645752,"value":"\nReduction : small view to big view","type":"text","attrs":{"fontSize":31,"color":"$5","fontWeight":"normal","textAlign":"center","verticalAlign":"center","padding":0,"isTitle":false}},{"t